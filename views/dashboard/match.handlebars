<h2 class="neon" id="dashH2">DateSpot</h2>
<div class="wrapper">
    <div class="profileCard">
        <figure class="profile-card-img">
<!--            <img src="{{match.images.profilePic}}">-->
            <img src="../../public/images/temp_pro_pics/user_pro_pic.png">
        </figure>
        <h2>{{match.firstName}}</h2>
        <div class="profile-card-info">
            <dl class="dl">
                <div class="dlDiv">
                    <dt>Location</dt>
                    <dd>{{match.location.city}}, {{match.location.principalSubdiv}}</dd>
                </div>
                <div class="dlDiv">
                    <dt>Age</dt>
                    <dd>{{match.age}}</dd>
                </div>
                <div class="dlDiv">
                    {{#if (showOnProfile match.showGender)}}
                        <dt>Gender</dt>
                        <dd>{{match.gender}}</dd>
                    {{/if}}
                </div>
                <div class="dlDiv">
                    {{#if (showOnProfile match.showPronouns)}}
                        <dt>Pronouns</dt>
                        <dd>{{match.pronouns}}</dd>
                    {{/if}}
                </div>
                <div class="dlDiv">
                    {{#if (aboutExists match.about)}}
                        <dt>About</dt>
                        <dd>{{match.about}}</dd>
                    {{/if}}
                </div>
                <div class="dlDiv">
                    <dt>Interests</dt>
                    <dd>{{stringifyObject match.interests}}</dd>

                </div>
            </dl>
            <div class="machButton">
                <button type="submit" class="homepage-buttons">Message</button>
            </div>
        </div>
    </div>
    <div class="dateSpotsContainer">

    </div>
</div>
<script src="/public/js/dashboard/match.js"></script>